<tr>
	<td>
    <% if(module_result.children.empty?) then
          icon = "file.png" 
        else 
          icon = "folder.png"
        end %>
    <%= image_tag(icon, :size => "20x16") %>
    <%= link_to module_result.module.name, project_repository_module_path(repository.project_id, repository.id, module_result.id) %>
  </td>
	<td><%= module_result.module.granularity %></td>
	<td><%= format_grade(module_result.grade) %></td>
</tr>